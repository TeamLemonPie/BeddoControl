<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="mainPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" spacing="10.0" AnchorPane.bottomAnchor="14.0"
              AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="25.0">
                    <children>
                        <Label text="Master-Section:">
                            <font>
                                <Font name="System Bold" size="14.0"/>
                            </font>
                        </Label>
                        <Button fx:id="buttonMasterLock" mnemonicParsing="false" text="Lock">
                            <font>
                                <Font name="System Bold" size="13.0"/>
                            </font>
                        </Button>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Button fx:id="buttonManageReaders" mnemonicParsing="false" onAction="#manageReaders"
                                text="Manage Readers">
                            <font>
                                <Font name="System Bold" size="13.0"/>
                            </font>
                        </Button>
                        <Region HBox.hgrow="ALWAYS"/>
                        <VBox alignment="CENTER">
                            <children>
                                <Label fx:id="labelConnectedBeddoFabriks">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Label>
                                <Label text="connected"/>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER">
                            <children>
                                <Label fx:id="labelStatus">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="labelServer"/>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER">
                            <children>
                                <Label fx:id="labelStatusMIDI" text="MIDI">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Label>
                                <Label/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <Separator prefWidth="200.0"/>
                <VBox fx:id="vboxAll" spacing="10.0" VBox.vgrow="ALWAYS">
                    <children>
                        <VBox spacing="15.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Players"
                                       underline="true">
                                    <font>
                                        <Font name="System Bold" size="16.0"/>
                                    </font>
                                </Label>
                                <VBox fx:id="vboxPlayerTable" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS"/>
                                <HBox alignment="CENTER" spacing="25.0">
                                    <children>
                                        <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#newPlayer"
                                                text="Add Player">
                                            <font>
                                                <Font name="System Bold" size="13.0"/>
                                            </font>
                                        </Button>
                                        <Button fx:id="buttonNewRound" mnemonicParsing="false" onAction="#newRound"
                                                text="New Round">
                                            <font>
                                                <Font name="System Bold" size="13.0"/>
                                            </font>
                                        </Button>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <Separator prefWidth="200.0"/>
                        <VBox alignment="TOP_CENTER" spacing="15.0">
                            <children>
                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Board"
                                       underline="true">
                                    <font>
                                        <Font name="System Bold" size="16.0"/>
                                    </font>
                                </Label>
                                <HBox fx:id="hboxBoard" alignment="CENTER" spacing="25.0">
                                    <children>
                                        <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#1">
                                                    <font>
                                                        <Font name="System Bold" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <ImageView fx:id="imageViewBoard1" fitHeight="39.0" fitWidth="51.0"
                                                           pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                                                    <image>
                                                        <Image url="@../../../../public/cards/back.png"/>
                                                    </image>
                                                </ImageView>
                                                <TextField fx:id="textFieldBoard1" maxWidth="50.0" minWidth="50.0"
                                                           prefHeight="25.0" prefWidth="50.0"/>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#2">
                                                    <font>
                                                        <Font name="System Bold" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <ImageView fx:id="imageViewBoard2" fitHeight="39.0" fitWidth="51.0"
                                                           pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                                                    <image>
                                                        <Image url="@../../../../public/cards/back.png"/>
                                                    </image>
                                                </ImageView>
                                                <TextField fx:id="textFieldBoard2" maxWidth="50.0" minWidth="50.0"
                                                           prefHeight="25.0" prefWidth="50.0"/>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#3">
                                                    <font>
                                                        <Font name="System Bold" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <ImageView fx:id="imageViewBoard3" fitHeight="39.0" fitWidth="51.0"
                                                           pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                                                    <image>
                                                        <Image url="@../../../../public/cards/back.png"/>
                                                    </image>
                                                </ImageView>
                                                <TextField fx:id="textFieldBoard3" maxWidth="50.0" minWidth="50.0"
                                                           prefHeight="25.0" prefWidth="50.0"/>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#4">
                                                    <font>
                                                        <Font name="System Bold" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <ImageView fx:id="imageViewBoard4" fitHeight="39.0" fitWidth="51.0"
                                                           pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                                                    <image>
                                                        <Image url="@../../../../public/cards/back.png"/>
                                                    </image>
                                                </ImageView>
                                                <TextField fx:id="textFieldBoard4" maxWidth="50.0" minWidth="50.0"
                                                           prefHeight="25.0" prefWidth="50.0"/>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#5">
                                                    <font>
                                                        <Font name="System Bold" size="14.0"/>
                                                    </font>
                                                </Label>
                                                <ImageView fx:id="imageViewBoard5" fitHeight="39.0" fitWidth="51.0"
                                                           pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                                                    <image>
                                                        <Image url="@../../../../public/cards/back.png"/>
                                                    </image>
                                                </ImageView>
                                                <TextField fx:id="textFieldBoard5" maxWidth="50.0" minWidth="50.0"
                                                           prefHeight="25.0" prefWidth="50.0"/>
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER" spacing="25.0">
                                    <children>
                                        <Button fx:id="buttonClearBoard" mnemonicParsing="false" onAction="#clearBoard"
                                                text="Clear">
                                            <font>
                                                <Font name="System Bold" size="13.0"/>
                                            </font>
                                        </Button>
                                        <Button fx:id="buttonLockBoard" mnemonicParsing="false" text="Lock">
                                            <font>
                                                <Font name="System Bold" size="13.0"/>
                                            </font>
                                        </Button>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <Separator prefWidth="200.0"/>
                        <HBox spacing="14.0" fx:id="hboxGameSettings"/>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>

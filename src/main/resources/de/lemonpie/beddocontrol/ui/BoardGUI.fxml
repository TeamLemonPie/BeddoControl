<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox fx:id="vboxRoot" alignment="TOP_CENTER" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Board" underline="true">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <HBox fx:id="hboxBoard" alignment="CENTER" spacing="25.0">
            <children>
                <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#1">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="imageViewBoard0" fitHeight="39.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                            <image>
                                <Image url="@../../../../public/cards/back.png" />
                            </image>
                        </ImageView>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#2">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="imageViewBoard1" fitHeight="39.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                            <image>
                                <Image url="@../../../../public/cards/back.png" />
                            </image>
                        </ImageView>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#3">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="imageViewBoard2" fitHeight="39.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                            <image>
                                <Image url="@../../../../public/cards/back.png" />
                            </image>
                        </ImageView>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#4">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="imageViewBoard3" fitHeight="39.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                            <image>
                                <Image url="@../../../../public/cards/back.png" />
                            </image>
                        </ImageView>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="92.0" prefWidth="79.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="#5">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="imageViewBoard4" fitHeight="39.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                            <image>
                                <Image url="@../../../../public/cards/back.png" />
                            </image>
                        </ImageView>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" spacing="25.0">
            <children>
                <Button fx:id="buttonClearBoard" mnemonicParsing="false" onAction="#clearBoard" text="Clear">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Button>
                <Button fx:id="buttonLockBoard" mnemonicParsing="false" text="Lock">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox spacing="14.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="vboxPause" alignment="CENTER" prefHeight="108.0" prefWidth="262.0" spacing="15.0"
              HBox.hgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Pause" underline="true">
                    <font>
                        <Font name="System Bold" size="16.0"/>
                    </font>
                </Label>
                <HBox alignment="CENTER" spacing="14.0">
                    <children>
                        <VBox alignment="CENTER" prefWidth="274.0" spacing="10.0">
                            <children>
                                <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                                               text="Pause time (min):" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <TextField fx:id="textFieldPause" prefHeight="0.0" prefWidth="75.0"/>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                                               prefHeight="20.0" text="Remaining:" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <Label fx:id="labelPause" prefWidth="75.0" text="--:--">
                                            <font>
                                                <Font name="System Bold" size="16.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <VBox spacing="10.0">
                            <children>
                                <Button fx:id="buttonPause" mnemonicParsing="false" onAction="#setPause">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="buttonPauseReset" mnemonicParsing="false" onAction="#resetPause">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
        <VBox fx:id="vboxNextPause" alignment="CENTER" prefHeight="108.0" prefWidth="289.0" spacing="15.0"
              HBox.hgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="next Pause" underline="true">
                    <font>
                        <Font name="System Bold" size="16.0"/>
                    </font>
                </Label>
                <HBox alignment="CENTER" spacing="14.0">
                    <children>
                        <VBox alignment="CENTER" prefWidth="274.0" spacing="10.0">
                            <children>
                                <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                                               text="time to next pause (min):" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <TextField fx:id="textFieldNextPause" prefHeight="0.0" prefWidth="75.0"/>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308"
                                               prefHeight="20.0" text="Remaining:" HBox.hgrow="ALWAYS">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <Label fx:id="labelNextPause" prefWidth="75.0" text="--:--">
                                            <font>
                                                <Font name="System Bold" size="16.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <VBox spacing="10.0">
                            <children>
                                <Button fx:id="buttonNextPause" mnemonicParsing="false" onAction="#setNextPause">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="buttonNextPauseReset" mnemonicParsing="false" onAction="#resetNextPause">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
    </children>
</HBox>

